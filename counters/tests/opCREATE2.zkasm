INCLUDE "../initIncludes.zkasm"

start:
    1000000 => GAS

operation:
    1 => CTX
    50              :MSTORE(storageAddr)
    50 => A
    0 => B, C
    10000 => D
    $ => SR             :SSTORE
    43       :MSTORE(SP++)
    54       :MSTORE(SP++)
    1000     :MSTORE(SP++)
            :JMP(opCREATE2)
checkCounters:

0 => A,B,C,D,E,CTX, SP, PC, GAS,  SR, HASHPOS, RR ; Set all registers to 0
finalizeExecution:
                                                                        :JMP(finalWait)
readCode:
txType:
    :JMP(checkCounters)
failedCounters:
2 => A
1       :ASSERT
INCLUDE "../endIncludes.zkasm"