INCLUDE "../initIncludes.zkasm"

start:
    1000000 => GAS

operation:
    1 => D
    2       :HASHK(0)
    2000    :MSTORE(SP++)
    100     :MSTORE(SP++)
            :JMP(opSHA3)
checkCounters:
3300 - STEP:JMPN(failedCounters)
146 - CNT_BINARY :JMPNZ(failedCounters)
9 - CNT_ARITH :JMPNZ(failedCounters)
15 - CNT_KECCAK_F :JMPNZ(failedCounters)
63 - CNT_MEM_ALIGN :JMPNZ(failedCounters)
%OPSHA3_CNT_PADDING_PG - CNT_PADDING_PG :JMPNZ(failedCounters)
%OPSHA3_CNT_POSEIDON_G - CNT_POSEIDON_G :JMPNZ(failedCounters)

0 => A,B,C,D,E,CTX, SP, PC, GAS, MAXMEM, SR, HASHPOS, RR ; Set all registers to 0
finalizeExecution:
                                                                        :JMP(finalWait)
readCode:
    :JMP(checkCounters)
failedCounters:
2 => A
1       :ASSERT
INCLUDE "../endIncludes.zkasm"