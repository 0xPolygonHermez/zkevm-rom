    0 => A,B,C,D,E,CTX, SP, PC, GAS,  SR, HASHPOS, RR ; Set all registers to 0
                                                                        :JMP(finalizeExecution)

initLoop:
    $${rounds = 32}
    0 => B ; to allow $$

loopAdd32Byte:
    $${rounds = rounds - 1}
    ${(A >> (rounds * 8)) & 0xFF} => E   :HASHP1(0)
    ${rounds}                            :JMPZ(endLoop, loopAdd32Byte)

endLoop:
