start:
    ; ----------
    ; "bytecode": "dead",
    ; "expectedHash": "0x2549d1fb0dc984e3098f235473637bd9e40aab1692c87e0afaf58720d2fbb8cd"
    2 => D
    0xdeadn => A
    A                :HASHP(0)
    HASHPOS          :HASHPLEN(0)
    $ => B           :HASHPDIGEST(0)
    0x2549d1fb0dc984e3098f235473637bd9e40aab1692c87e0afaf58720d2fbb8cdn => A
    B                :ASSERT

    ; ----------
    ; "bytecode": "34665289b71a2cb8bf4c289ae6d17d845457c48bfc18623ca39e141b2e40c5d3",
    ; "expectedHash": "0x26aa5d09e2046f5ab7e311b32c6e34fa52a6dc8257a34b494af84fe1471c589c
    0 => HASHPOS
    32 => D
    0x34665289b71a2cb8bf4c289ae6d17d845457c48bfc18623ca39e141b2e40c5d3n => A
    A                :HASHP(1)
    HASHPOS          :HASHPLEN(1)
    $ => B           :HASHPDIGEST(1)
    0x26aa5d09e2046f5ab7e311b32c6e34fa52a6dc8257a34b494af84fe1471c589cn => A
    B                :ASSERT

    ; insert two bytes and read afterwards one
    0 => HASHPOS
    1 => D
    ; add first byte
    0x11 => A
    A                :HASHP(2)
    ; add second byte
    0x22 => A
    A                :HASHP(2)
    ; finish hash
    HASHPOS          :HASHPLEN(2)
    $ => B           :HASHPDIGEST(2)

    ; read first byte form hashP(2)
    0 => HASHPOS
    1 => D
    $ => A           :HASHP(2)
    0x11             :ASSERT

    $ => A           :HASHP(2)
    0x22             :ASSERT

    0 => A,B,C,D,E,CTX, SP, PC, GAS, MAXMEM, SR, HASHPOS, RR                 ; Set all registers to 0

finalWait:
    ${beforeLast()}  : JMPN(finalWait)
                     : JMP(start)