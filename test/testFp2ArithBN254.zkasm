        5150174793373947111116801351825241197340308507599487920632714798740053225549n   :ASSERT
        D => A
        1287543698343486777779200337956310299335077126899871980158178699685013306387n   :ASSERT

        %BN254_P => A
        0n => B
                                                                                        :CALL(invFp2BN254)
