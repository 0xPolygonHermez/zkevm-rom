INCLUDE "../main/constants.zkasm"

start:
    %MAX_CNT_STEPS => A
    ${log(A, steps)}

    %MAX_CNT_ARITH => A
    ${log(A, arith)}

    %MAX_CNT_BINARY => A
    ${log(A, binary)}

    %MAX_CNT_MEM_ALIGN => A
    ${log(A, memAlign)}

    %MAX_CNT_KECCAK_F => A
    ${log(A, keccak)}

    %MAX_CNT_PADDING_PG => A
    ${log(A, Padding)}

    %MAX_CNT_POSEIDON_G => A
    ${log(A, Poseidon)}

    %MAX_CNT_SHA256_F => A
    ${log(A, Sha256)}

    ; label finalizeExecution needed by executor C++
finalizeExecution:
        ${beforeLast()}  :JMPN(finalizeExecution)
                         :JMP(start)