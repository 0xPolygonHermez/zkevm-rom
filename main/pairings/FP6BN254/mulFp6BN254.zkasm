        $ => A          :MLOAD(mulFp6BN254_b1_x)
        $ => B          :MLOAD(mulFp6BN254_b1_y)
        $ => C          :MLOAD(mulFp6BN254_b2_x)
        $ => D          :MLOAD(mulFp6BN254_b2_y), CALL(addFp2BN254)
        E               :MSTORE(mulFp6BN254_b1b2sum_x)
