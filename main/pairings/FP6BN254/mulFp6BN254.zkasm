        $ => B          :MLOAD(mulFp6BN254_a1a3sum_y)
        $ => C          :MLOAD(mulFp6BN254_b1b3sum_x)
        $ => D          :MLOAD(mulFp6BN254_b1b3sum_y), CALL(mulFp2BN254)

        E => A
