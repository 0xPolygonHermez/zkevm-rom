;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; double_out (256bits) = double_in * 2
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

VAR GLOBAL double_in
VAR GLOBAL double_out
VAR GLOBAL tmpA
VAR GLOBAL tmpB

double:
        A :MSTORE(tmpA)
        B :MSTORE(tmpB)

        ${dump(mem.double_in)}

        $ => A,B :MLOAD(double_in)
        $ :ADD,MSTORE(double_out)

        $ => A :MLOAD(tmpA)
        $ => B :MLOAD(tmpA)

        ${dump(mem.double_out)}
        :RETURN
