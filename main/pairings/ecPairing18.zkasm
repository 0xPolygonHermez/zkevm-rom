VAR GLOBAL ecPairing18_P2_y
VAR GLOBAL ecPairing18_Q2_x1
VAR GLOBAL ecPairing18_Q2_x2
VAR GLOBAL ecPairing18_Q2_y1
VAR GLOBAL ecPairing18_Q2_y2
VAR GLOBAL ecPairing18_P3_x
VAR GLOBAL ecPairing18_P3_y
VAR GLOBAL ecPairing18_Q3_x1
VAR GLOBAL ecPairing18_Q3_x2
VAR GLOBAL ecPairing18_Q3_y1
VAR GLOBAL ecPairing18_Q3_y2

VAR GLOBAL ecPairing18_P1Q1_f11_x
VAR GLOBAL ecPairing18_P1Q1_f11_y
VAR GLOBAL ecPairing18_P1Q1_f12_x
VAR GLOBAL ecPairing18_P1Q1_f12_y
VAR GLOBAL ecPairing18_P1Q1_f13_x
VAR GLOBAL ecPairing18_P1Q1_f13_y
VAR GLOBAL ecPairing18_P1Q1_f21_x
VAR GLOBAL ecPairing18_P1Q1_f21_y
VAR GLOBAL ecPairing18_P1Q1_f22_x
VAR GLOBAL ecPairing18_P1Q1_f22_y
VAR GLOBAL ecPairing18_P1Q1_f23_x
VAR GLOBAL ecPairing18_P1Q1_f23_y
VAR GLOBAL ecPairing18_P2Q2_f11_x
VAR GLOBAL ecPairing18_P2Q2_f11_y
VAR GLOBAL ecPairing18_P2Q2_f12_x
VAR GLOBAL ecPairing18_P2Q2_f12_y
VAR GLOBAL ecPairing18_P2Q2_f13_x
VAR GLOBAL ecPairing18_P2Q2_f13_y
VAR GLOBAL ecPairing18_P2Q2_f21_x
VAR GLOBAL ecPairing18_P2Q2_f21_y
VAR GLOBAL ecPairing18_P2Q2_f22_x
VAR GLOBAL ecPairing18_P2Q2_f22_y
VAR GLOBAL ecPairing18_P2Q2_f23_x
VAR GLOBAL ecPairing18_P2Q2_f23_y
VAR GLOBAL ecPairing18_P3Q3_f11_x
VAR GLOBAL ecPairing18_P3Q3_f11_y
VAR GLOBAL ecPairing18_P3Q3_f12_x
VAR GLOBAL ecPairing18_P3Q3_f12_y
VAR GLOBAL ecPairing18_P3Q3_f13_x
VAR GLOBAL ecPairing18_P3Q3_f13_y
VAR GLOBAL ecPairing18_P3Q3_f21_x
VAR GLOBAL ecPairing18_P3Q3_f21_y
VAR GLOBAL ecPairing18_P3Q3_f22_x
VAR GLOBAL ecPairing18_P3Q3_f22_y
VAR GLOBAL ecPairing18_P3Q3_f23_x
VAR GLOBAL ecPairing18_P3Q3_f23_y

VAR GLOBAL ecPairing18_mul_f11_x
VAR GLOBAL ecPairing18_mul_f11_y
VAR GLOBAL ecPairing18_mul_f12_x
VAR GLOBAL ecPairing18_mul_f12_y
VAR GLOBAL ecPairing18_mul_f13_x
VAR GLOBAL ecPairing18_mul_f13_y
VAR GLOBAL ecPairing18_mul_f21_x
VAR GLOBAL ecPairing18_mul_f21_y
VAR GLOBAL ecPairing18_mul_f22_x
