preEnd:
    $ => B          :MLOAD(gasCTX)
    B + GAS => GAS
    $ => SP         :MLOAD(lastSP)
    $ => PC         :MLOAD(lastPC)
    1               :MSTORE(SP++)
    ${checkpoint()}
                    :JMP(readCode)

preEndFail:
    $ => GAS        :MLOAD(gasCTX)
    $ => SP         :MLOAD(lastSP)
    $ => PC         :MLOAD(lastPC)
    0               :MSTORE(SP++)
    ${revert()}
                    :JMP(readCode)