;; PRE: A is not alias-free
;; POST: The result B is not alias-free (on MAP)
;;
;; invFpSecp256k1:
;;             in: A
;;             out: B = A⁻¹ (mod p)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 4 steps

invFpSecp256k1:

        ; A·A⁻¹ + 0 = 1 (mod SECP256K1_P)
        ${var _invFpSecp256k1_A = inverseFpEc(A)} => B
        0 => C
        %SECP256K1_P => D
        1n                      :ARITH_MOD, RETURN