;; PRE: A,C are not alias-free
;; POST: The result C is alias-free
;;
;; addFpSecp256k1:
;;             in: A,C
;;             out: C = A + C (mod p)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 3 steps

addFpSecp256k1:

        ; AÂ·1 + C = op (mod SECP256K1_P)
        1 => B
        %SECP256K1_P => D
        ${(A + C) % D} => C   :ARITH_MOD, RETURN