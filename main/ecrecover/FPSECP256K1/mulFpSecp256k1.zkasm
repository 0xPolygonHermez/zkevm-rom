;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; PRE: A,B are not alias-free
;; POST: The result C is alias-free
;;
;; mulFpSecp256k1:
;;             in: A,B
;;             out: C = A·B (mod p)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 4 steps

mulFpSecp256k1:

        ; A·B + 0 = op (mod SECP256K1_P)
        0 => C
        %SECP256K1_P => D
        ${(A*B + C) % D} => E   :ARITH_MOD

        E => C                  :RETURN