;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; PRE: C is not alias-free
;; POST: The result C is alias-free
;;
;; squareFpSecp256k1:
;;             in: C
;;             out: C = C·C (mod p)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 5 steps

squareFpSecp256k1:

        ; C·C + 0 = op (mod SECP256K1_P)
        C => A,B
        0 => C
        %SECP256K1_P => D
        ${(A*B + C) % D} => E   :ARITH_MOD

        E => C                  :RETURN