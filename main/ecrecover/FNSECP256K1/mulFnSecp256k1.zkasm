;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; PRE: A,B are not alias-free
;; POST: The result C is alias-free
;;
;; mulFnSecp256k1:
;;             in: A,B
;;             out: C = A·B (mod n)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 4 steps

mulFnSecp256k1:

        ; A·B + 0 = op (mod SECP256K1_N)
        0 => C
        %SECP256K1_N => D
        ${(A*B + C) % D} => E   :ARITH_MOD

        E => C                  :RETURN