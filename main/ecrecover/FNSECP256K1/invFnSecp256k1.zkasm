;; PRE: A is not alias-free
;; POST: The result B is not alias-free (on MAP)
;;
;; invFnSecp256k1:
;;             in: A
;;             out: B = A⁻¹ (mod n)
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; RESOURCES:
;   1 arith + 4 steps

invFnSecp256k1:

        ; A·A⁻¹ + 0 = 1 (mod SECP256K1_N)
        ${var _invFnSecp256k1_A = inverseFnEc(A)} => B
        0 => C
        %SECP256K1_N => D
        1n                      :ARITH_MOD, RETURN