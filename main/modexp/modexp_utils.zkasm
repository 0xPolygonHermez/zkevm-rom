
; @info return Einit without 0's
; @in E => Einit with 0's
; @out E => Einit without 0's

VAR GLOBAL modexp_getInit_RR

modexp_getInit:
    RR                  :MSTORE(modexp_getInit_RR)
    1 => C

modexp_getInit2:
                        :CALL(MLOADX)
    31 => D             :CALL(SHRarith)
    A                   :JMPZ(modexp_getInit2)
    E - 1 => E
    $ => RR             :MLOAD(modexp_getInit_RR)
                        :RETURN